/* pages/searchlist/searchlist.wxss */
.goodcontainer{
  display: flex;
  flex-wrap: wrap;
}
.good{
  width: 50%;
  padding: 20rpx;
  box-sizing: border-box;
  text-align: center;
}
.good image{
  width: 400rpx;

}
.name{
  font-size: 35rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  /* 设置对齐模式 */
  -webkit-box-orient: vertical; /*设置对其模式*/
  -webkit-line-clamp: 2;/*设置多行的行数*/
}